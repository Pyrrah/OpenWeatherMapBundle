{
    "name": "pyrrah/openweathermap-bundle",
    "description": "Pyrrah OpenWeatherMap Bundle for Symfony",
    "keywords": ["pyrrah", "openweathermap", "open", "weather", "map", "symfony", "bundle"],
    "homepage": "https://github.com/pyrrah/PyrrahOpenWeatherMapBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre-Yves Dick",
            "email": "contact@pyrrah.eu",
            "homepage": "http://www.pyrrah.eu"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Pyrrah\\Bundle\\OpenWeatherMapBundle\\": [ "src/", "tests/" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.1-dev"
        }
    }
